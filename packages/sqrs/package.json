{
  "name": "@brainhubeu/sqrs",
  "version": "1.0.1",
  "description": "CQRS framework for JavaScript/TypeScript servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project ./tsconfig.json 'src/**/*'",
    "prebuild": "rimraf dist",
    "build": "tsc --outDir dist",
    "predoc": "rimraf ../../docs/api",
    "doc": "typedoc --options typedoc.json --out ../../docs/api",
    "doc2": "typedoc --mode modules --readme none --theme markdown --project tsconfig.json --exclude '**/errors/*' --excludePrivate --out ../../docs/api"
  },
  "keywords": [
    "cqrs",
    "cqs",
    "ddd",
    "command",
    "query",
    "repository",
    "segregation",
    "domain",
    "driven",
    "event"
  ],
  "author": {
    "name" : "Rafa≈Ç Kostrzewski",
    "url" : "https://github.com/rkostrzewski"
  },
  "repository": {
    "url": "http://github.com/brainhubeu/sqrs",
    "type": "git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typedoc-plugin-markdown": "^1.1.25",
    "typescript": "^3.1.6"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
